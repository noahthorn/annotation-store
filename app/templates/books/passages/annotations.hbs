<div id="annotations-list">
	{{#paper-toolbar}}
	{{#paper-toolbar-tools}}
	<h2>Annotations</h2>
	{{/paper-toolbar-tools}}
	{{/paper-toolbar}}
	{{#paper-list}}
	{{#each @model.annotations as |annotation|}}
	{{#paper-item}}
	<p>{{annotation.text}}</p>
	{{/paper-item}}
	{{/each}}
	{{/paper-list}}
</div>